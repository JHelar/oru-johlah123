//Header för Multimedia
#pragma once

#include <iostream>
#include <string>

using namespace std;

class Multimedia{
private:
	string name,date,type;
public:
	Multimedia();
	Multimedia(string n,string d, string t);
	void setMedia(string n,string d, string t);
	void setName(string name){ this->type=type;}
	void setDate(string date){ this->date=date;}
	void setType(string type){ this->type=type;}
	string getName() const { return name; }
	string getDate() const { return date; }
	string getType() const { return type; }

	friend ostream& operator<< (ostream &out, Multimedia &m);
	friend istream& operator>> (istream &in, Multimedia &m);
	friend bool operator < (Multimedia &one, Multimedia &another);
	friend bool operator == (Multimedia &one, Multimedia &another);
};



